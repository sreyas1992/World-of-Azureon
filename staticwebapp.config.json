{
  "routes": [
    {
      "route": "/",
      "serve": "/index.html"
    },
    {
      "route": "/level2",
      "serve": "/level2.html"
    },
    {
      "route": "/level3",
      "serve": "/level3_game.html"
    },
    {
      "route": "/level4",
      "serve": "/level4_game.html"
    },
    {
      "route": "/*.png",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "route": "/*.css",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "mimeTypes": {
    ".png": "image/png",
    ".css": "text/css",
    ".html": "text/html",
    ".js": "application/javascript"
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "X-XSS-Protection": "1; mode=block"
  }
}